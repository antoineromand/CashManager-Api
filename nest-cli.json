{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/monolith/src",
  "projects": {
    "entities": {
      "type": "library",
      "root": "libs/entities",
      "entryFile": "index",
      "sourceRoot": "libs/entities/src",
      "compilerOptions": {
        "tsConfigPath": "libs/entities/tsconfig.lib.json"
      }
    },
    "monolith": {
      "type": "application",
      "root": "apps/monolith",
      "entryFile": "main",
      "sourceRoot": "apps/monolith/src",
      "compilerOptions": {
        "tsConfigPath": "apps/monolith/tsconfig.app.json"
      }
    },
    "bank": {
      "type": "application",
      "root": "apps/bank",
      "entryFile": "main",
      "sourceRoot": "apps/bank/src",
      "compilerOptions": {
        "tsConfigPath": "apps/bank/tsconfig.app.json"
      }
    },
    "company": {
      "type": "application",
      "root": "apps/company",
      "entryFile": "main",
      "sourceRoot": "apps/company/src",
      "compilerOptions": {
        "tsConfigPath": "apps/company/tsconfig.app.json"
      }
    },
    "app": {
      "type": "application",
      "root": "apps/app",
      "entryFile": "main",
      "sourceRoot": "apps/app/src",
      "compilerOptions": {
        "tsConfigPath": "apps/app/tsconfig.app.json"
      }
    },
    "auth": {
      "type": "application",
      "root": "apps/auth",
      "entryFile": "main",
      "sourceRoot": "apps/auth/src",
      "compilerOptions": {
        "tsConfigPath": "apps/auth/tsconfig.app.json"
      }
    },
    "utils": {
      "type": "library",
      "root": "libs/utils",
      "entryFile": "index",
      "sourceRoot": "libs/utils/src",
      "compilerOptions": {
        "tsConfigPath": "libs/utils/tsconfig.lib.json"
      }
    }
  },
  "compilerOptions": {
    "webpack": true,
    "tsConfigPath": "apps/monolith/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/monolith"
}